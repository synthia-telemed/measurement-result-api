generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model doctors {
  id         BigInt   @id @default(autoincrement())
  created_at DateTime
  updated_at DateTime
  ref_id     String   @unique
}

model patients {
  id         BigInt   @id @default(autoincrement())
  created_at DateTime
  updated_at DateTime
  ref_id     String   @unique
  birth_date DateTime
}
